<configuration>
    <property>
        <name>hadoop.tmp.dir</name>
        <value>/opt/hadoop/dfs/temp</value>
    </property>

    <property>
        <name>fs.defaultFS</name>
        <value>hdfs://hadoop-cluster</value>
        <final>true</final>
    </property>

    <property>
        <name>ha.zookeeper.quorum</name>
        <value>nn01:2181,nn02:2181,dn01:2181</value>
    </property>
</configuration>